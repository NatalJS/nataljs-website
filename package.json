{
  "name": "nataljs.github.io",
  "version": "1.1.0",
  "description": "Natal JS user group website",
  "main": "index.js",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "export": "next export",
    "jest": "jest --config ./tests/jest.config.js",
    "test": "npm run jest -- --coverage",
    "start": "NODE_ENV=production && npm run build && npm run export && http-server ./out -o",
    "deploy": "rm -r out && npm run build && npm run export && cd public && git ls-files -z| xargs -0 git rm -fr && rm -rf _next || true && cp -r ../out/* . && touch .nojekyll && git add -A && git commit -m 'updating website' && git push origin master && cd .."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NatalJS/nataljs.github.io.git"
  },
  "keywords": [
    "natal-rn",
    "javascript",
    "frontend",
    "nataljs"
  ],
  "author": "NatalJS",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/NatalJS/nataljs.github.io/issues"
  },
  "homepage": "https://github.com/NatalJS/nataljs.github.io#readme",
  "dependencies": {
    "bootstrap": "^4.1.1",
    "http-server": "^0.11.1",
    "jest": "^24.0.0",
    "next": "^7.0.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "scroll-navigation-menu": "git+https://github.com/jhonnymichel/scroll-navigation-menu"
  },
  "devDependencies": {
    "@zeit/next-sass": "^1.0.1",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.0.0",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-plugin-module-resolver": "^3.1.3",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.8.0",
    "eslint": "^5.12.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.0",
    "eslint-plugin-react": "^7.12.4",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "webpack": "4.23.1"
  }
}
